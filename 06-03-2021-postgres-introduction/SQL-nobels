CREATE TABLE IF NOT EXISTS nobels (
  id SERIAL PRIMARY KEY,
  laureate_id INTEGER REFERENCES laureates,
  award_year SMALLINT,
  category TEXT NOT NULL,
  category_fullname TEXT NOT NULL,
  date_awarded TIMESTAMP,
  prize_status TEXT,
  motivation TEXT NOT NULL,
  prize_amount INTEGER NOT NULL,
  affiliations TEXT
);

